Last date modified: 11 November 2022
"""

print('Hello, lets play guessing number game!')
import random
n = random.randrange(1,10)
guess = int(input("Enter any number: "))
while n!= guess:
    if guess < n:
        print("Nope, Too low")
        guess = int(input("Enter number again: "))
    elif guess > n:
        print("Nope, Too high!")
        guess = int(input("Enter number again: "))
    else:
      break
print("congrats, you won!!")
